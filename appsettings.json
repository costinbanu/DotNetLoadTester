{
  //Mandatory. The base url to crawl
  "BaseUrl": "",

  //Mandatory. Max number of parallel threads
  "MaxParallelThreads": 400,

  //Mandatory. Max Session duration, in minutes. 
  "MaxDurationMinutes": 5,

  //Optional. URLs matching this pattern will be crawled. An empty pattern will result in crawling all URLs starting with the Base URL
  "IncludePatern": "",

  //Optional. URLs matching this pattern will not be crawled
  "ExcludePattern": ".*\\.css|.*\\.js|.*\\.ico"
}